# find factorial number of given numbers


no=5
fact =1
for i in range(1,no+1):
    fact=fact*i
print(fact)



# Reverse integer number

n1 = 6789
rev = 0

while n1 != 0:
    i = n1 % 10
    rev = rev * 10 + i
    n1 =n1// 10

print(6789)
print(rev)

# number is palindrom or not (any number it will after reversing 121, 121)

n2 = 121
rev2 = 0
n3=n2
while n2 != 0:
    i = n2 % 10
    rev2 = rev2 * 10 + i
    n2 =n2// 10
print(rev2)

if n3==rev2:
    print("number is Palindrom")
else: 
    print("number is not palindrom")


# Armstrong number

n2 = 153
rev2 = 0
n3=n2
while n2 != 0:
    i = n2 % 10
    rev2 = rev2+i**3       # need to change logic here
    n2 =n2// 10
print(rev2)

if n3==rev2:
    print("number is ArmStrong")
else: 
    print("number is not ArmStrong")



