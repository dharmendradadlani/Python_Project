# count the digit of the number
l2=124502
count=0

while l2!=0:
    l2=l2//10
    count=count+1
print(count)


# print numnber

# 5   4   3   2   1
# 4   3   2   1
# 3   2   1
# 2   1
# 1


i=5
j=5


for  i in range(0,i+1):
    for k in range (j-i,0,-1):
        print(k, end=' ')
    print()


